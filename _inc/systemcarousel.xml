<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<view name="system">
		<carousel name="systemcarousel">
			<zIndex>1000</zIndex>
			<color>202020</color>
			<color ifSubset="mode:transparent">10101080</color>
			<colorEnd ifSubset="mode:transparent">20202080</colorEnd>
			<color ifSubset="mode:translucent">10101066</color>
			<colorEnd ifSubset="mode:translucent">20202066</colorEnd>
			<gradientType ifSubset="mode:transparent|translucent">vertical</gradientType>
			<defaultTransition>instant</defaultTransition>
			<scrollSound>./sounds/scroll.wav</scrollSound>
		</carousel>
	</view>
</theme>
