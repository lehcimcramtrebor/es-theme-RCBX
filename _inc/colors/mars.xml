<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<variables>
		<!--themable colors-->
		<c.text>cececc</c.text>

		<c.textHeader>cccc50</c.textHeader>

		<c.menuSelector>808030</c.menuSelector>
		<ce.menuSelector>707020</ce.menuSelector>

		<c.scroll>70702080</c.scroll>

			<!--frames colors-->
			<c.framePlain>282820ef</c.framePlain>

			<c.frameTransparent>18181080</c.frameTransparent>
			<ce.frameTransparent>28282080</ce.frameTransparent>

			<c.frameTranslucent>18181066</c.frameTranslucent>
			<ce.frameTranslucent>28282066</ce.frameTranslucent>

			<!--menu background color-->
			<c.menubg>282820</c.menubg>
	</variables>
	<include>./fixedColors.xml</include>
	<subset name="frameColorset" displayName="${lang.frameColorset}">
        <include name="yes" displayName="${lang.yes}"/>
        <include name="no" displayName="${lang.no}">./frameColors.xml</include>
    </subset>
</theme>
